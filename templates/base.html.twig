<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>{% block title %}Agence Site{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22> </text></svg>">
    <link rel="stylesheet" href="{{ asset('css/base.css') }}">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
  </head>
  <body>
    {% block header %}
      <header>
      </header>
    {% endblock %}

      {% block body %}
      {% endblock %}

    {% block footer %}
      <footer>
        <div class="footer-container">
            <div class="footer-section logo">
                <img src="{{ asset('images/Trip_advice.png') }}" alt="Trip Advice Logo">
            </div>
            <div class="footer-section follow">
                <h3>Suivez-nous</h3>
                <ul>
                    <li><a href="#">Tiktok</a></li>
                    <li><a href="#">Youtube</a></li>
                    <li><a href="#">Instagram</a></li>
                    <li><a href="#">Facebook</a></li>
                </ul>
            </div>

            <div class="footer-section contact">
                <h3>Contactez-nous</h3>
                <ul>
                    <li><a href="#">Nous contacter</a></li>
                    <li><a href="#">Centre médiatique</a></li>
                </ul>
            </div>

            <div class="footer-section info">
                <h3>Informations</h3>
                <ul>
                    <li><a href="#">Conditions générales</a></li>
                    <li><a href="#">Avis de confidentialité</a></li>
                    <li><a href="#">Site Internet officiel</a></li>
                    <li><a href="#">Nous contacter</a></li>
                </ul>
            </div>
        </div>

        <div class="footer-bottom">
            <p>Copyright © 2024 Trip Advice | Tous droits réservés</p>
        </div>
      </footer>
    {% endblock %}
  </body>
</html> 